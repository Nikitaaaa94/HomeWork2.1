import java.awt.*;
import java.sql.SQLOutput;
import java.util.Scanner;

public class Employee {

    String fullName;
    String profession;
    String email;
    String mobile;
    int salary;
    int age;

    public Employee(String fullName, String profession, String email,String mobile, int salary, int age){
        this.fullName = fullName;
        this.profession = profession;
        this.email = email;
        this.mobile = mobile;
        this.salary = salary;
        this.age = age;
    }
    @Override
    public String toString(){
        return String.format("Имя: %s \tДолжность: %s \nEmail: %s \t Номер телефона: %s \n Зарплата: %d \t Возраст: %d \n",
                fullName, profession, email, mobile, salary, age);}

    public void print(){
        System.out.println(this);
    }
}